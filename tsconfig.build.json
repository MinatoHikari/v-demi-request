{
    "compilerOptions": {
        "target": "esnext",
        "module": "esnext",
        "moduleResolution": "node",
        "strict": true,
        "jsx": "preserve",
        "sourceMap": true,
        "resolveJsonModule": true,
        "esModuleInterop": true,
        "declaration": true,
        "lib": ["esnext", "dom"],
        "outDir": "./dist",
        "emitDeclarationOnly": true,
        "skipLibCheck": true
    },
    "include": ["src/v-demi-request/*.ts", "src/v-demi-request/*.d.ts", "src/v-demi-request/*.tsx"]
}
